<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
      <!-- Allow to download packages from nuget.org. -->
      <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
      <!-- Allow to download internal packages from our internal nuget feed -->
      <add key="nuget internal" value="http://TODO:_internal_nuget_server_hostname/api/v2" />
      <!-- Allow to download internal packages that may not be pushed to our internal nuget feed. (prerelease packages?) -->
	    <add key="Teamcity" value="http://TODO:teamcity_server_hostname/guestAuth/app/nuget/v1/FeedService.svc/" />
  </packageSources>
</configuration>
